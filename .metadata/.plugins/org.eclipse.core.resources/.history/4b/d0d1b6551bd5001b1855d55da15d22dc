//Using arrays
public class Exercises {
	
	public static void main(String[] args) {
		//Prepopulated array
		//it's still an object
		int i;
		for (i = 0; i<=10; ++i ) {
			
		
		System.out.println(i);
		}
		//i ++;
	/*do while example 
	 * 
	 * 	do {
			System.out.println(i);
			i++;
		}while (i<3);
	}*/
	}	
}

