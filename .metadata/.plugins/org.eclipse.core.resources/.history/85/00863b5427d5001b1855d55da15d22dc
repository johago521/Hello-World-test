import java.util.Arrays;
//Create a while loop that stores the numbers
//one through fifty in an array.
public class Exercises {
	
	public static void main(String[] args) {
		
		int[] nums = new int[50];
		int counter = 1;
		int i = 0;
		
		while (i < nums.length){
			nums[i] = counter;
			i++;
			counter++;
		}
			System.out.println(Arrays.toString(nums));
		
	}
}

